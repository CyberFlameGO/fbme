<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE FBType SYSTEM 'http://www.holobloc.com/xml/LibraryElement.dtd'>
<FBType Name="DrillController">
  <Identification Standard="61499-2" />
  <InterfaceList>
    <EventInputs>
      <Event Name="INIT">
        <With Var="UP" />
        <With Var="DOWN" />
      </Event>
      <Event Name="CHG">
        <With Var="UP" />
        <With Var="DOWN" />
      </Event>
      <Event Name="DR_EV"></Event>
    </EventInputs>
    <EventOutputs>
      <Event Name="CHGO">
        <With Var="LIFT" />
        <With Var="DRILL" />
        <With Var="LOCK" />
      </Event>
    </EventOutputs>
    <InputVars>
      <VarDeclaration Name="UP" Type="BOOL" />
      <VarDeclaration Name="DOWN" Type="BOOL" />
    </InputVars>
    <OutputVars>
      <VarDeclaration Name="LIFT" Type="BOOL" />
      <VarDeclaration Name="DRILL" Type="BOOL" />
      <VarDeclaration Name="LOCK" Type="BOOL" />
    </OutputVars>
  </InterfaceList>
  <BasicFB>
    <InternalVars></InternalVars>
    <ECC>
      <ECState Name="START" x="0.0" y="0.0"></ECState>
      <ECState Name="INIT" x="0.0" y="0.0">
        <ECAction Algorithm="LIFT_1" />
      </ECState>
      <ECState Name="WAITING" x="0.0" y="0.0">
        <ECAction Algorithm="LIFT_0" />
        <ECAction Algorithm="LOCK_0" Output="CHGO" />
      </ECState>
      <ECState Name="DRILL" x="0.0" y="0.0">
        <ECAction Algorithm="DRILL_1" />
        <ECAction Algorithm="LOCK_1" Output="CHGO" />
      </ECState>
      <ECState Name="LIFTING" x="0.0" y="0.0">
        <ECAction Algorithm="LIFT_1" />
        <ECAction Algorithm="DRILL_0" />
      </ECState>
      <ECTransition Source="START" Destination="INIT" Condition="INIT[NOT UP]" x="0.0" y="0.0" />
      <ECTransition Source="INIT" Destination="WAITING" Condition="[UP]" x="0.0" y="0.0" />
      <ECTransition Source="START" Destination="WAITING" Condition="INIT[UP]" x="0.0" y="0.0" />
      <ECTransition Source="WAITING" Destination="DRILL" Condition="DR_EV" x="0.0" y="0.0" />
      <ECTransition Source="DRILL" Destination="LIFTING" Condition="[DOWN]" x="0.0" y="0.0" />
      <ECTransition Source="LIFTING" Destination="WAITING" Condition="[UP]" x="0.0" y="0.0" />
    </ECC>
    <Algorithm Name="LIFT_0">
      <ST Text="LIFT := FALSE;&#38;#10;" />
    </Algorithm>
    <Algorithm Name="LIFT_1">
      <ST Text="LIFT := TRUE;&#38;#10;" />
    </Algorithm>
    <Algorithm Name="DRILL_0">
      <ST Text="DRILL := FALSE;&#38;#10;" />
    </Algorithm>
    <Algorithm Name="DRILL_1">
      <ST Text="DRILL := TRUE;&#38;#10;" />
    </Algorithm>
    <Algorithm Name="LOCK_0">
      <ST Text="LOCK := FALSE;&#38;#10;" />
    </Algorithm>
    <Algorithm Name="LOCK_1">
      <ST Text="LOCK := TRUE;&#38;#10;" />
    </Algorithm>
  </BasicFB>
</FBType>