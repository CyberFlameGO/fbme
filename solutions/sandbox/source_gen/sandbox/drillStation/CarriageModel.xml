<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE FBType SYSTEM 'http://www.holobloc.com/xml/LibraryElement.dtd'>
<FBType Name="CarriageModel">
  <Identification Standard="61499-2" />
  <InterfaceList>
    <EventInputs>
      <Event Name="CHGI">
        <With Var="INSIDE" />
        <With Var="OUTSIDE" />
      </Event>
    </EventInputs>
    <EventOutputs>
      <Event Name="CHANGE">
        <With Var="LOAD_P" />
        <With Var="WORK_P" />
        <With Var="COORD" />
      </Event>
    </EventOutputs>
    <InputVars>
      <VarDeclaration Name="INSIDE" Type="BOOL" />
      <VarDeclaration Name="OUTSIDE" Type="BOOL" />
    </InputVars>
    <OutputVars>
      <VarDeclaration Name="COORD" Type="DINT" />
      <VarDeclaration Name="LOAD_P" Type="BOOL" />
      <VarDeclaration Name="WORK_P" Type="BOOL" />
    </OutputVars>
  </InterfaceList>
  <BasicFB>
    <InternalVars></InternalVars>
    <ECC>
      <ECState Name="START" x="0.0" y="0.0">
        <ECAction Algorithm="LOAD_P_1" />
        <ECAction Algorithm="WORK_P_0" />
      </ECState>
      <ECState Name="PROCESSING" x="0.0" y="0.0">
        <ECAction Algorithm="LOAD_P_0" />
        <ECAction Algorithm="WORK_P_0" Output="CHANGE" />
      </ECState>
      <ECState Name="WAITING" x="0.0" y="0.0">
        <ECAction Algorithm="LOAD_P_1" />
        <ECAction Algorithm="WORK_P_1" Output="CHANGE" />
      </ECState>
      <ECTransition Source="START" Destination="PROCESSING" Condition="CHGI[INSIDE]" x="0.0" y="0.0" />
      <ECTransition Source="PROCESSING" Destination="WAITING" Condition="CHGI[OUTSIDE]" x="0.0" y="0.0" />
      <ECTransition Source="WAITING" Destination="PROCESSING" Condition="CHGI[INSIDE]" x="0.0" y="0.0" />
    </ECC>
    <Algorithm Name="LOAD_P_0">
      <ST Text="LOAD_P := FALSE;&#38;#10;" />
    </Algorithm>
    <Algorithm Name="LOAD_P_1">
      <ST Text="LOAD_P := TRUE;&#38;#10;" />
    </Algorithm>
    <Algorithm Name="WORK_P_0">
      <ST Text="WORK_P := FALSE;&#38;#10;" />
    </Algorithm>
    <Algorithm Name="WORK_P_1">
      <ST Text="WORK_P := TRUE;&#38;#10;" />
    </Algorithm>
  </BasicFB>
</FBType>