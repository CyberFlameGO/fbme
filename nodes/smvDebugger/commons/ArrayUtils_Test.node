test case ArrayUtils extends <none> {
  <<members>>

  <<before test>>

  <<after test>>


  test toArray {
    assert true Arrays.equals(new Object[]{1}, ArrayUtils.toArray(1));
  }


  test to2dArray {
    final Object[][] result = ArrayUtils.to2dArray(new Object[]{1, 2});
    assert true Arrays.equals(new Object[]{1}, result[0]);
    assert true Arrays.equals(new Object[]{2}, result[1]);
  }


  test concatObjects {
    assert true Arrays.equals(new Object[]{1, 2, 3}, ArrayUtils.concat(1, new Object[]{2, 3}));
  }


  test concatStrings {
    assert true Arrays.equals(new string[]{"1", "2", "3"}, ArrayUtils.concat(new string[]{"1", "2"}, "3"));
  }


  test subarray {
    assert true Arrays.equals(new string[]{"2", "3"}, ArrayUtils.subarray(new string[]{"1", "2", "3", "4"}, 1, 3));
  }
}